<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".glycosetime">

    <TextView
        android:id="@+id/textView13"
        android:layout_width="242dp"
        android:layout_height="45dp"
        android:layout_marginEnd="71dp"
        android:layout_marginLeft="71dp"
        android:layout_marginRight="71dp"
        android:layout_marginStart="71dp"
        android:layout_marginTop="40dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="What time did you check your glycose?" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="108dp"
        android:layout_marginLeft="109dp"
        android:layout_marginRight="108dp"
        android:layout_marginStart="109dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="179dp"
        tools:text="What is your glycose level?" />

    <EditText
        android:id="@+id/glycoselevel"
        android:layout_width="89dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="147dp"
        android:layout_marginLeft="148dp"
        android:layout_marginRight="147dp"
        android:layout_marginStart="148dp"
        android:layout_marginTop="31dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <Button
        android:id="@+id/btn_Done"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="149dp"
        android:layout_marginLeft="147dp"
        android:layout_marginRight="149dp"
        android:layout_marginStart="147dp"
        android:layout_marginTop="170dp"
        android:text="Done"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/glycoselevel" />

    <Spinner
        android:id="@+id/spinnerHour"
        android:layout_width="39dp"
        android:layout_height="44dp"
        tools:layout_editor_absoluteX="109dp"
        tools:layout_editor_absoluteY="116dp" />

    <Spinner
        android:id="@+id/spinnerAMPM"
        android:layout_width="39dp"
        android:layout_height="44dp"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="116dp" />

    <Spinner
        android:id="@+id/spinnerMin"
        android:layout_width="39dp"
        android:layout_height="44dp"
        tools:layout_editor_absoluteX="71dp"
        tools:layout_editor_absoluteY="116dp" />

    <Spinner
        android:id="@+id/spinnerDay"
        android:layout_width="39dp"
        android:layout_height="44dp"
        tools:layout_editor_absoluteX="221dp"
        tools:layout_editor_absoluteY="116dp" />

    <Spinner
        android:id="@+id/spinnerMonth"
        android:layout_width="39dp"
        android:layout_height="44dp"
        tools:layout_editor_absoluteX="274dp"
        tools:layout_editor_absoluteY="116dp" />

</android.support.constraint.ConstraintLayout>